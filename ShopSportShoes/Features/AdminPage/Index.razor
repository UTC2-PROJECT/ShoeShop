@page "/admin"
@attribute [Authorize(Roles = "Admin")]
@inject GoogleDriveService DriveService
@inject NavigationManager NavigationManager

@using ShopSportShoes.Features.AdminPage.Components

@code {

    protected override void OnInitialized()
    {
        try
        {
            NavigationManager.NavigateTo("/admin/product-management");
        }
        catch(Exception e)
        {
            Console.WriteLine(e.Message);
        }
        base.OnInitialized();
    }
}
